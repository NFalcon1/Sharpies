<Window x:Class="Graphing_Calculator_UI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Graphing_Calculator_UI"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid Margin="0,0,-8,-8">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="314*"/>
            <ColumnDefinition Width="203*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="4*"/>
            <RowDefinition Height="315*"/>
        </Grid.RowDefinitions>
        <Button x:Name="Graph" Content="Graph" HorizontalAlignment="Left" Margin="118.483,3,0,0" VerticalAlignment="Top" Width="75" Height="30" Grid.Row="1" Grid.Column="1"/>
        <Button x:Name="MyButton" Content="Cancel" HorizontalAlignment="Left" Margin="118.483,44,0,0" VerticalAlignment="Top" Width="75" Height="30" Grid.Row="1" Grid.Column="1"/>
        <Label x:Name="label3" Content="Y = " HorizontalAlignment="Left" Margin="17,52,0,0" Grid.Row="1" VerticalAlignment="Top" Width="76"/>
        <TextBox x:Name="FunctionBox" HorizontalAlignment="Left" Height="23" Margin="103,55,0,0" Grid.Row="1" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>

    </Grid>
</Window>
